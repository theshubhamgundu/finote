{
  "expo": {
    "name": "Finote",
    "slug": "finote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/images/favicon.png",
    "scheme": "finote",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shubhamgundu.finote",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/images/adaptive-icon.png",
        "backgroundColor": "#171717"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shubhamgundu.finote"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./public/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./public/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#171717"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "31e19f61-270e-4d8d-bd95-1af9d39bd933"
      }
    }
  }
}
